<table class="table table-dark table-hover">
    <thead>
        <tr>
            <th>ID</th>
            <th>Category Name</th>
            <th>Description</th>
            <th>Pic</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var a in ViewBag.SubCategory)
        {
            <tr>
                <td>@a.SubCategoryId</td>
                <td>@a.SubCategoryName</td>
                <td>@a.Description</td>
                <td><img style="width:100px" src="~/SubCatPic/@a.Pic" /></td>
            </tr>

        }
    </tbody>
</table>